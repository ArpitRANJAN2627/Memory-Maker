
<%- include('./partials/header')  %> 

<main class="container   my-4">
    <div class="row products">
 <% for(const image of images) {%>
    <div class="col-lg-3 col-md-6 mb-3">
        <div class="card shadow-sm mx-auto position-relative" style="width: 20rem;">
            <img src="<%=image.imgUrl%>" class="card-img-top" alt="item image">
          
            <div class="card-body">
              <h5 class="card-title d-flex justify-content-between align-items-center">
                <%= image.title %>
              </h5>
              <p class="card-text fw-light"><%= image.des.substring(0,40)%> ...</p>
             
            
              <a href="/item/<%= image._id %>" class="btn btn-sm btn-primary">Visit</a>
            </div>
        </div>
    </div>
<% } %>
</div>
</main>
<%- include('./partials/footer')  %> 